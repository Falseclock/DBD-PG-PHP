{
  "name": "falseclock/dbd-php",
  "type": "library",
  "description": "Database driver for PHP (PostgreSQL, MySQL, MSSQL, OData, YellowERP, 1ะก)",
  "keywords": [
    "dbd",
    "dbi",
    "sql",
    "cache",
    "driver",
    "postgre",
    "PostgreSQL",
    "MySQL",
    "MSSQL",
    "Microsoft SQL Server",
    "OData",
    "1ะก",
    "1C",
    "YellowERP",
    "Yellow ERP"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Nurlan Mukhanov",
      "email": "nurike@gmail.com"
    }
  ],
  "require": {
    "php": "^7.1",
    "psr/simple-cache": "*",
    "openlss/lib-array2xml": "^1",
    "falseclock/dbd-php-common": "dev-master",
    "falseclock/dbd-php-entity": "dev-master"
  },
  "require-dev": {
    "ext-pgsql": "*",
    "ext-memcache": "*",
    "phpunit/phpunit": "^7.5.20",
    "php-coveralls/php-coveralls": "^2.4.2"
  },
  "suggest": {
    "ext-memcache": "Required to support Memcached",
    "ext-pgsql": "Required to support PostgreSQL database"
  },
  "autoload": {
    "psr-4": {
      "DBD\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DBD\\Tests\\": "tests/"
    }
  }
}
